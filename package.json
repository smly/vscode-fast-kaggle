{
  "name": "fastkaggle",
  "displayName": "FastKaggle",
  "description": "VS Code Extension for Kaggle",
  "publisher": "smly",
  "icon": "images/fastkaggle_128x128.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/smly/vscode-fast-kaggle"
  },
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "FastKaggle",
      "properties": {
        "fastkaggle.executablePath": {
          "type": "string",
          "default": "",
          "description": "Absolute path to the Kaggle executable"
        }
      }
    },
    "languages": [
      {
        "id": "json",
        "extensions": [
          ".json"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "commands": [
      {
        "command": "fastkaggle.listCompetitions",
        "title": "Kaggle: List Competitions"
      },
      {
        "command": "fastkaggle.newDataset",
        "title": "Kaggle: New Dataset"
      },
      {
        "command": "fastkaggle.newNotebook",
        "title": "Kaggle: New Notebook"
      },
      {
        "command": "fastkaggle.updateDatasetOrNotebook",
        "title": "Kaggle: Update Dataset/Notebook"
      },
      {
        "command": "fastkaggle.getStatus",
        "title": "Kaggle: Get Status"
      },
      {
        "command": "fastkaggle.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "fastkaggle.clipboardSlugName",
        "title": "Copy Slug Name",
        "icon": "$(clippy)"
      },
      {
        "command": "fastkaggle.openInBrowser",
        "title": "Open in Browser"
      },
      {
        "command": "fastkaggle.showItemStatusFromTreeView",
        "title": "Show status"
      }
    ],
    "keybindings": [
      {
        "command": "fastkaggle.updateDatasetOrNotebook",
        "key": "ctrl+shift+u",
        "mac": "ctrl+shift+u"
      },
      {
        "command": "fastkaggle.getStatus",
        "key": "ctrl+shift+k",
        "mac": "ctrl+shift+k"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "kaggleItemView",
          "name": "Kaggle",
          "contextualTitle": "Kaggle"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "fastkaggle.refresh",
          "when": "false"
        },
        {
          "command": "fastkaggle.openInBrowser",
          "when": "false"
        },
        {
          "command": "fastkaggle.clipboardSlugName",
          "when": "false"
        },
        {
          "command": "fastkaggle.showItemStatusFromTreeView",
          "when": "false"
        },
        {
          "command": "fastkaggle.getStatus",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "fastkaggle.refresh",
          "when": "view == kaggleItemView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "fastkaggle.clipboardSlugName",
          "when": "view == kaggleItemView",
          "group": "inline"
        },
        {
          "command": "fastkaggle.openInBrowser",
          "when": "view == kaggleItemView"
        },
        {
          "command": "fastkaggle.showItemStatusFromTreeView",
          "when": "view == kaggleItemView"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@types/cacheable-request": "^6.0.3",
    "ajv": "^8.17.1",
    "jsonc-parser": "^3.3.1",
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12"
  }
}
